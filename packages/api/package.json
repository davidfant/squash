{
  "name": "@squashai/api",
  "type": "module",
  "version": "0.0.3",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib/resolveMessageThreadHistory": {
      "types": "./dist/lib/resolveMessageThreadHistory.d.ts",
      "default": "./dist/lib/resolveMessageThreadHistory.js"
    },
    "./agent/types": {
      "types": "./dist/agent/types.d.ts",
      "default": "./dist/agent/types.js"
    },
    "./sandbox/types": {
      "types": "./dist/sandbox/types.d.ts",
      "default": "./dist/sandbox/types.js"
    }
  },
  "scripts": {
    "dev": "pnpm typegen && concurrently \"pnpm dev:wrangler\" \"pnpm dev:tsc\"",
    "dev:wrangler": "pnpm drizzle-kit push && wrangler dev",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "drizzle:migrate:dev": "drizzle-kit push",
    "drizzle:migrate:prod": "DOTENV_CONFIG_PATH=.env.production drizzle-kit migrate",
    "push": "wrangler deploy --env production",
    "playground": "tsx --env-file .dev.vars playground.ts",
    "build": "tsc -b",
    "typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/google": "^2.0.0",
    "@ai-sdk/openai": "^2.0.3",
    "@ai-sdk/provider": "^2.0.0",
    "@daytonaio/sdk": "^0.108.0",
    "@hono/zod-validator": "^0.7.1",
    "@neondatabase/serverless": "^1.0.1",
    "@octokit/auth-app": "^8.0.2",
    "@octokit/rest": "^22.0.0",
    "@opentelemetry/context-async-hooks": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@squashai/ai-sdk-claude-code": "workspace:*",
    "ai": "^5.0.5",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.2.12",
    "drizzle-orm": "^0.44.2",
    "eventsource-parser": "^3.0.5",
    "hono": "^4.9.0",
    "jsonlines": "^0.1.1",
    "jsonwebtoken": "^9.0.2",
    "langsmith": "^0.3.53",
    "limiter": "^3.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "mime": "^4.1.0",
    "openai": "^5.21.0",
    "shell-escape": "^0.2.0",
    "typescript": "~5.8.3",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250918.0",
    "@types/jsonlines": "^0.1.5",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/lodash.upperfirst": "^4.3.9",
    "@types/shell-escape": "^0.2.3",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "wrangler": "^4.42.0"
  }
}
