{
  "name": "@squashai/replicator-git-sync",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm typegen && concurrently \"pnpm dev:tsx\" \"pnpm dev:tsc\"",
    "dev:tsx": "tsx --watch --env-file=.env src/index.ts",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "_push": "wrangler deploy --env production",
    "typegen": "wrangler types --env-interface CloudflareBindings",
    "build": "tsc -b"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@cloudflare/containers": "^0.0.25",
    "@hono/node-server": "^1.18.1",
    "@hono/zod-validator": "^0.7.1",
    "hono": "^4.9.0",
    "simple-git": "^3.28.0",
    "tar": "^7.4.3",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "concurrently": "^9.2.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "wrangler": "^4.31.0"
  }
}
