{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "mcp-composio",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-10",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "dev": {
    "port": 8791
  },
  "durable_objects": {
    "bindings": [{ "name": "MCP_OBJECT", "class_name": "ComposioMcpAgent" }]
  },
  "migrations": [{ "tag": "v1", "new_sqlite_classes": ["ComposioMcpAgent"] }]
}
