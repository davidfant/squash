{
  "name": "@squashai/flyio-ssh-proxy",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm typegen && concurrently \"pnpm dev:tsx\" \"pnpm dev:tsc\"",
    "dev:tsx": "tsx --watch --env-file=.env src/index.ts",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "push": "wrangler deploy --env production",
    "typegen": "wrangler types --env-interface CloudflareBindings",
    "build": "tsc -b"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.0.25",
    "@hono/node-server": "^1.19.0",
    "@hono/zod-validator": "^0.7.2",
    "concurrently": "^9.2.1",
    "hono": "^4.9.4",
    "shell-escape": "^0.2.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/shell-escape": "^0.2.3",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "wrangler": "^4.40.0"
  }
}
