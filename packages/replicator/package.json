{
  "name": "@squash/replicator",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./metadata/reactFiber": {
      "types": "./dist/metadata/reactFiber.d.ts",
      "default": "./dist/metadata/reactFiber.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:tsc\" \"pnpm dev:alias\"",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:alias": "tsc-alias --watch",
    "build": "tsc",
    "replicate": "tsx watch --env-file .env src/playground.ts",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:vitest": "vitest"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@squash/chrome-extension": "workspace:*",
    "@types/cheerio": "^1.0.0",
    "@types/estree": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-reconciler": "^0.32.0",
    "aws4fetch": "^1.0.20",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-jest": "^29.4.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "util": "^0.12.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/openai": "^2.0.3",
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
    "ai": "^5.0.9",
    "cheerio": "^1.1.2",
    "estree-util-visit": "^2.0.0",
    "hast-util-to-html": "^9.0.5",
    "hastscript": "^9.0.1",
    "jest-diff": "^30.0.5",
    "nanotar": "^0.2.0",
    "parse5": "^8.0.0",
    "prettier": "^3.0.0",
    "recma-jsx": "^1.0.1",
    "recma-stringify": "^1.0.0",
    "rehype-minify-whitespace": "^6.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-recma": "^1.0.0",
    "rehype-remove-comments": "^6.1.1",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "zod": "^4.0.16"
  }
}
