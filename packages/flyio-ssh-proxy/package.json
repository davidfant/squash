{
  "name": "@squash/flyio-ssh-proxy",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm typegen && concurrently \"pnpm dev:tsx\" \"pnpm dev:tsc\"",
    "dev:tsx": "tsx --watch --env-file=.env src/index.ts",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "push": "wrangler deploy",
    "typegen": "wrangler types --env-interface CloudflareBindings",
    "build": "tsc -b"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.0.25",
    "concurrently": "^9.2.1",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "wrangler": "^4.31.0"
  }
}
